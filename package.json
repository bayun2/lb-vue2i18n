{
  "name": "lb-vue2i18n",
  "displayName": "lb-vue2i18n",
  "publisher": "rwt",
  "icon": "icon.jpeg",
  "description": "",
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.lbVue2i18n",
    "onCommand:extension.lbJS2i18n",
    "onCommand:extension.lbDiffCNWithEN",
    "onCommand:extension.lbDiffCNWithHK",
    "onCommand:extension.lbExportArchive"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.lbVue2i18n",
        "title": "I18n 提取 Vue 内的中文"
      },
      {
        "command": "extension.lbJS2i18n",
        "title": "I18n 提取 JS/TS/React 内的中文"
      },
      {
        "command": "extension.lbDiffCNWithEN",
        "title": "I18n 提取英文语言包比中文语言包缺少的词条"
      },
      {
        "command": "extension.lbDiffCNWithHK",
        "title": "I18n 提取繁体语言包比中文语言包缺少的词条"
      },
      {
        "command": "extension.lbExportArchive",
        "title": "I18n 下载该项目多语言文件"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "extension.lbVue2i18n",
          "group": "I18n Extract"
        },
        {
          "when": "editorFocus",
          "command": "extension.lbJS2i18n",
          "group": "I18n Extract"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "jest",
    "publish": "vsce publish",
    "package": "vsce package"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "babel-jest": "^28.1.3",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "fetch": "^1.1.0",
    "form-data": "^4.0.0",
    "global": "^4.4.0",
    "jest": "^28.1.3",
    "loco-api-js": "^1.2.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1"
  }
}