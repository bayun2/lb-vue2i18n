{
  "name": "lb-vue2i18n",
  "displayName": "lb-vue2i18n",
  "publisher": "rwt",
  "icon": "icon.jpeg",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.lbVue2i18n",
    "onCommand:extension.lbJS2i18n",
    "onCommand:extension.lbDiffCNWithEN",
    "onCommand:extension.lbDiffCNWithHK",
    "onCommand:extension.lbExportArchive"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.lbVue2i18n",
        "title": "lb: 提取 vue 文件内的中文"
      },
      {
        "command": "extension.lbJS2i18n",
        "title": "lb: 提取 js 文件内的中文"
      },
      {
        "command": "extension.lbDiffCNWithEN",
        "title": "lb: 提取英文语言包比中文语言包缺少的词条"
      },
      {
        "command": "extension.lbDiffCNWithHK",
        "title": "lb: 提取繁体语言包比中文语言包缺少的词条"
      },
      {
        "command": "extension.lbExportArchive",
        "title": "lb: 下载该项目多语言文件"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "extension.lbVue2i18n"
        },
        {
          "when": "editorFocus",
          "command": "extension.lbJS2i18n"
        },
        {
          "command": "extension.lbDiffCNWithEN"
        },
        {
          "command": "extension.lbDiffCNWithHK"
        },
        {
          "command": "extension.lbExportArchive"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "fetch": "^1.1.0",
    "form-data": "^4.0.0",
    "global": "^4.4.0",
    "loco-api-js": "^1.2.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1"
  }
}
